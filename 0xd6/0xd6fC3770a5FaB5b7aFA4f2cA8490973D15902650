,2.)>.5||c.z==0.?0.:step(0.,B(v))),1./G),0.,1.);}"].map(((e,a)=>attachShader(p,s=createShader(35633-a),shaderSource(s,"#version 300 es~V vec2~W vec3~X vec4~F float~U uniform~T texture~G X(2.2)~P pow~O V u;void main()\\nprecision highp F;\\n".split`~`.join`\\n#define `+e),compileShader(s)))),linkProgram(p),useProgram(p),U=(e,a,r)=>G["uniform"+e](getUniformLocation(p,a),r),B=(e,a=e)=>activeTexture(33984+e)||bindTexture(t=3553,x[a]),(A=e=>requestAnimationFrame((e=>(P&&F||(F%i||(a=(r=0|R(+O.g+1))/(w<=h?w:h),m=w*a+.5|0||1,n=h*a+.5|0||1,U("1i","t",1),B(1,2),T(m,n,new Float32Array([...Array(m*n)].flatMap((e=>[R(),R(),R()>.01*O.i*!!r&&R()**.5,(R()<.5)<<1|R()<.01*O.c]))))),U("4fv","k",[...C[(a=F/i|0)%3],...C[++a%3],m,n,F%i/i,F/i]),[0,1].map((e=>(U("1i","b",e),B(0,a=F/i&1^e),J(e?null:y[1-a]),drawArrays(5,0,3)))),F++),A()))))(P=0,I())</script></canvas>'];
    uint256 private _tokenIdCounter;
    struct Traits {
        uint256 gridWidth;
        uint256 feedbackInterval;
        uint256 idleCells;
        uint256 clampedCells;
    }

    event SaleActive(bool _active);

    error InactiveSale();
    error SoldOut();
    error InvalidEthAmount();
    error InvalidSupply();
    error ZeroBalance();

    constructor() ERC721("COMMIT", "CMT") Ownable(msg.sender) {
        withdrawalAddress = payable(msg.sender);
        _setDefaultRoyalty(payable(msg.sender), 500);
    }

    /// @notice Mint token to caller.
    /// @dev Fail when saleActive is false.
    /// function mint() external payable nonReentrant {
    function mint() external payable nonReentrant {
        if (_tokenIdCounter >= maxSupply) revert SoldOut();
        if (!saleActive) revert InactiveSale();
        if (msg.value != mintPrice) revert InvalidEthAmount();
        tokenSeed[_tokenIdCounter] = _createHash(_tokenIdCounter);
        _safeMint(msg.sender, _tokenIdCounter);
        ++_tokenIdCounter;
    }

    /// @notice Mint free token to contract owner.
    /// @dev Can only be called by the contract owner.
    function creatorMint() external onlyOwner {
        if (_tokenIdCounter >= maxSupply) revert SoldOut();
        tokenSeed[_tokenIdCounter] = _createHash(_tokenIdCounter);
        _safeMint(msg.sender, _tokenIdCounter);
        ++_tokenIdCounter;
    }

    /// @notice Set HTML htmlTemplate array.
    /// @param _part1 The template head string.
    /// @param _part2 The template tail string.
    /// @dev A string with the seed will be inserted between the two parts.
    function setHtmlTemplate(string memory _part1, string memory _part2) external onlyOwner {
        htmlTemplate = [_part1, _part2];
    }

    /// @notice Set saleActive boolean state.
    /// @param _active The boolean state value.
    function setSaleActive(bool _active) external onlyOwner {
        saleActive = _active;
        emit SaleActive(_active);
    }
    
    /// @notice Set collection max supply.
    /// @param _supply The new supply amount.
    /// @dev The new supply can only be inferior to previous one, and superior or equal to token count.
    function setMaxSupply(uint256 _supply) external onlyOwner {
        if (_supply < _tokenIdCounter || _supply >= maxSupply) revert InvalidSupply();
        maxSupply = _supply;
    }

    /// @notice Set tokens mint price.
    /// @param _price The new price.
    function setMintPrice(uint256 _price) external onlyOwner {
        mintPrice = _price;
    }

    /// @notice Set external_url metadata string prefix.
    /// @param _url The URL string to set `baseExternalUrl` to.
    function setBaseExternalUrl(string memory _url) external onlyOwner {
        baseExternalUrl = _url;
    }

    /// @notice Set image metadata string prefix.
    /// @param _url The URL string to set `baseImageUrl` to.
    function setBaseImageUrl(string memory _url) external onlyOwner {
        baseImageUrl = _url;
    }

    /// @notice Set withdrawal address.
    /// @param _address The address to set `withdrawalAddress` to.
    function setWithdrawalAddress(address payable _address) external onlyOwner {
        withdrawalAddress = _address;
    }

    /// @notice Withdraw all balance to withdrawalAddress.
    function withdraw() external onlyOwner nonReentrant {
        if (address(this).balance == 0) revert ZeroBalance();
        Address.sendValue(withdrawalAddress, address(this).balance);
    }

    function setDefaultRoyalty(address payable _receiver, uint96 _feeNumerator) external onlyOwner {
      _setDefaultRoyalty(_receiver, _feeNumerator);
    }

    /// @notice Return the contract balance in Wei.
    /// @return The resulting unsigned integer.
    function contractBalance() external view returns (uint256) {
        return address(this).balance;
    }

    /// @notice Return the amount of tokens created.
    /// @return The resulting unsigned integer.
    function totalSupply() external view returns (uint256) {
        return _tokenIdCounter;
    }

    /// @notice Generate token HTML string
    /// @param _tokenId The token ID.
    /// @return The resulting string.
    function tokenHTML(uint256 _tokenId) public view returns (string memory) {
        string memory seedString = uint256(tokenSeed[_tokenId]).toHexString();
        Traits memory traits = _generateTraits(_tokenId);
        string memory html = string.concat(htmlTemplate[0], 'O={'
            'S:"', seedString, '",'
            'g:', traits.gridWidth.toString(), ','
            'f:', traits.feedbackInterval.toString(), ','
            'i:', traits.idleCells.toString(), ','
            'c:', traits.clampedCells.toString(),
        '}', htmlTemplate[1]);
        return html;
    }

    /// @notice Generate token metadata string.
    /// @param _tokenId The token ID.
    /// @return A base64 encoded Data URL string.
    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        string memory idString = _tokenId.toString();
        string memory imageUrl = string.concat(baseImageUrl, idString);
        string memory externalUrl = string.concat(baseExternalUrl, idString);
        Traits memory traits = _generateTraits(_tokenId);
        string memory json = string.concat('{'
            '"name":"COMMIT #', idString, '",'
            '"id":', idString, ','
            '"author":"Unruly Unroll",'
            '"description":"[COMMIT #', idString, '](', externalUrl, '), '
            'on-chain generative artwork by Unruly Unroll, 2024. [https://commit.unrulyunroll.xyz](https://commit.unrulyunroll.xyz)\\n\\n'
            'Pause/Resume the animation with SPACE key or left-click.\\nSave a screenshot in PNG format with S key.\\n'
            'Enter fullscreen mode with F key or double-click.\\nExit fullscreen mode with ESC key.",'
            '"image":"', imageUrl, '",'
            '"external_url":"', externalUrl, '",'
            '"animation_url":"data:text/html;base64,', Base64.encode(bytes(tokenHTML(_tokenId))), '",');
        json = string.concat(json, '"attributes":['
            '{"trait_type":"Grid Width","value":"', traits.gridWidth.toString(), '"},'
            '{"trait_type":"Feedback Interval","value":"', traits.feedbackInterval.toString(), '"},'
            '{"trait_type":"Idle Cells (%)","value":"', traits.idleCells.toString(), '"},'
            '{"trait_type":"Clamped Cells (%)","value":"', traits.clampedCells.toString(), '"}'
        '],"seed":"', uint256(tokenSeed[_tokenId]).toHexString(), '"}');
        return string.concat("data:application/json;base64,", Base64.encode(bytes(json)));
    }
    
    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC2981) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /// @dev Create a hash for the given tokenId, blockNumber and sender.
    /// @param _tokenId The token ID.
    /// @return The resulting hash.
    function _createHash(uint256 _tokenId) private view returns (bytes32) {
        unchecked {return keccak256(abi.encodePacked(_tokenId, msg.sender, blockhash(block.number - 1), block.prevrandao, block.coinbase));}
    }

    /// @notice Return a Traits struct for a given token ID.
    /// @param _tokenId The token ID.
    /// @return t The resulting Traits struct.
    function _generateTraits(uint256 _tokenId) private view returns (Traits memory t) {
        uint256[] memory randomNumbers = _generateRandomNumbers(tokenSeed[_tokenId], 5);
        t.gridWidth = 3 + randomNumbers[0] % 3;
        uint256 a = randomNumbers[1] % 9;
        uint256 b = randomNumbers[2] % 9;
        t.feedbackInterval = 12 + (a < b ? a : b);
        t.idleCells = 40 + randomNumbers[3] % 41;
        t.clampedCells = 50 + randomNumbers[4] % 41;
    }

    /// @dev Generates an array of random numbers based on a seed value.
    /// @param seed The seed value.
    /// @param length The number of random numbers to generate.
    /// @return An array of random numbers with length equal to `length`.
    function _generateRandomNumbers(bytes32 seed, uint256 length) private pure returns (uint256[] memory) {
        uint256[] memory randNumbers = new uint256[](length);
        for (uint256 i; i < length; i++) {
            randNumbers[i] = uint256(keccak256(abi.encodePacked(uint256(seed) + i)));
        }
        return randNumbers;
    }
}

