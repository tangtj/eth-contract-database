contract HowMuchYouLikeBitCOINNow {
        
    event HowMuchYouLikeBitCoinNow(uint8 Between0AND255);

    function howMuchYouLikeBitCoinNow(uint8 _Between0And255) public {
        emit HowMuchYouLikeBitCoinNow(_Between0And255);
    }


}