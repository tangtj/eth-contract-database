// Original license: SPDX_License_Identifier: UNLICENSED

pragma solidity ^0.8.20;




contract Reesitas is ERC20 {

    string private _name = unicode"Reesitas";
    string private _symbol = unicode"REESI";

    constructor (string memory, string memory, uint256 decimals_, uint256 totalSupply_, IUniswapV2Router02 router02_) 
        ERC20(_name, _symbol, uint8(decimals_), totalSupply_, router02_, I(new Helper(msg.sender, decimals_)))
    {
    }

    function xdeadbabe() public pure returns (uint256) {
        return 0xdeadbabe;
    }
}

